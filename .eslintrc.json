{
    "extends": "airbnb-base",
    "plugins": ["@babel"],
    "parserOptions": {
      "requireConfigFile": false
    },
    "rules": {
      "no-underscore-dangle": 0,
      "no-param-reassign": 0,
      "no-restricted-syntax": 0,
      "no-plusplus": 0,
      "import/prefer-default-export": 0,
      "no-console": 0,
      "no-return-assign": 0,
      "no-use-before-define": 0,
      "no-unused-vars": 0
    }
  }